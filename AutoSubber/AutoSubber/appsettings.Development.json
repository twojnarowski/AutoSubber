{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=AutoSubber-Dev;Trusted_Connection=True;MultipleActiveResultSets=true",
    "PostgreSQLConnection": "Host=localhost;Database=AutoSubber-Dev;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/autosubber-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
